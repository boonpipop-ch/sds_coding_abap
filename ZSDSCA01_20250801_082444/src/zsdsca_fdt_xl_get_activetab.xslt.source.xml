<xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:s="http://schemas.openxmlformats.org/spreadsheetml/2006/main" xmlns:sap="http://www.sap.com/sapxsl" version="1.0">

  <xsl:strip-space elements="*"/>
  <xsl:template match="/">
    <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
      <asx:values>
        <ACTIVETAB>
          <xsl:value-of select="s:workbook/s:bookViews/s:workbookView/@activeTab"/>
        </ACTIVETAB>
      </asx:values>
    </asx:abap>
  </xsl:template>
</xsl:transform>
