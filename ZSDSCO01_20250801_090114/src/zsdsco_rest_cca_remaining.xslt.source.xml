<?sap.transform simple?>
<!--<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">-->
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary">
  <tt:root name="DATA" type="ddic:ZSDSCOS009"/>
  <tt:template>
    <object>

      <str name="K2RespCCAIn">
        <tt:value ref="DATA.PARAM_IN.K2_RESP_CCA"/>
      </str>
      <str name="K2BudgetYearIn">
        <tt:value ref="DATA.PARAM_IN.K2_BG_YEAR"/>
      </str>
      <str name="K2PeriodFromIn">
        <tt:value ref="DATA.PARAM_IN.K2_PERIOD_FR"/>
      </str>
      <str name="K2PeriodToIn">
        <tt:value ref="DATA.PARAM_IN.K2_PERIOD_TO"/>
      </str>
      <str name="K2GLAccountIn">
        <tt:value ref="DATA.PARAM_IN.K2_GL"/>
      </str>

      <!-- Response     -->
      <str name="ResponseStatus">
        <tt:value ref="DATA.RESP_STATUS"/>
      </str>

      <str name="ResponseMessage">
        <tt:value ref="DATA.RESP_MESSAGE"/>
      </str>


      <tt:cond check="exist(DATA.T_DATA)">
        <array name="Data">
          <tt:loop ref="DATA.T_DATA">
            <!-- Response-->
            <object>
              <str name="K2RespCCAOut">
                <tt:value ref="$ref.K2_RESP_CCA"/>
              </str>
              <str name="K2BudgetYearOut">
                <tt:value ref="$ref.K2_BG_YEAR"/>
              </str>
              <str name="K2PeriodFromOut">
                <tt:value ref="$ref.K2_PERIOD_FR"/>
              </str>
              <str name="K2PeriodToOut">
                <tt:value ref="$ref.K2_PERIOD_TO"/>
              </str>
              <str name="K2GLAccountOut">
                <tt:value ref="$ref.K2_GL"/>
              </str>
              <str name="CurrencyKeyOut">
                <tt:value ref="$ref.WAERS"/>
              </str>
              <str name="Period001">
                <tt:value ref="$ref.WOG001"/>
              </str>
              <str name="Period002">
                <tt:value ref="$ref.WOG002"/>
              </str>
              <str name="Period003">
                <tt:value ref="$ref.WOG003"/>
              </str>
              <str name="Period004">
                <tt:value ref="$ref.WOG004"/>
              </str>
              <str name="Period005">
                <tt:value ref="$ref.WOG005"/>
              </str>
              <str name="Period006">
                <tt:value ref="$ref.WOG006"/>
              </str>
              <str name="Period007">
                <tt:value ref="$ref.WOG007"/>
              </str>
              <str name="Period008">
                <tt:value ref="$ref.WOG008"/>
              </str>
              <str name="Period009">
                <tt:value ref="$ref.WOG009"/>
              </str>
              <str name="Period010">
                <tt:value ref="$ref.WOG010"/>
              </str>
              <str name="Period011">
                <tt:value ref="$ref.WOG011"/>
              </str>
              <str name="Period012">
                <tt:value ref="$ref.WOG012"/>
              </str>
              <str name="PeriodTOT">
                <tt:value ref="$ref.WOGTOT"/>
              </str>

            </object>
          </tt:loop>
        </array>
      </tt:cond>

      <tt:cond check="exist(DATA.RESPONSE)">
        <array name="Response">
          <tt:loop ref="DATA.RESPONSE">
            <!-- Response-->
            <object>
              <!--              <tt:group>-->
              <str name="Status">
                <tt:value ref="$ref.RESP_STATUS"/>
              </str>
              <str name="Message">
                <tt:value ref="$ref.RESP_MESSAGE"/>
              </str>
              <!--              </tt:group>-->
            </object>
          </tt:loop>
        </array>
      </tt:cond>

    </object>
  </tt:template>
</tt:transform>
